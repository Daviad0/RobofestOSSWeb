<style>
    .almostdone {
        animation-name: color;
        animation-duration: 1.5s;
        animation-iteration-count: infinite;
    }
    .status{
        border-bottom:12px gray solid;
        border-top:12px gray solid;
    }
    @@keyframes color {
        0% {
            border-bottom-color: green;
            border-top-color: green;
        }

        50% {
            border-bottom-color: orange;
            border-top-color: orange;
        }
    }
 .lds-ellipsis {
  display: inline-block;
  position: relative;
  width: 80px;
  height: 80px;
}
.lds-ellipsis div {
  position: absolute;
  top: 33px;
  width: 13px;
  height: 13px;
  border-radius: 50%;
  background: #000000;
  animation-timing-function: cubic-bezier(0, 1, 1, 0);
}
.lds-ellipsis div:nth-child(1) {
  left: 8px;
  animation: lds-ellipsis1 1.8s infinite;
  animation-delay: 1.5s;
}
.lds-ellipsis div:nth-child(2) {
  left: 8px;
  animation: lds-ellipsis2 1.8s infinite;
  animation-delay: 1.5s;
}
.lds-ellipsis div:nth-child(3) {
  left: 32px;
  animation: lds-ellipsis2 1.8s infinite;
  animation-delay: 1.5s;
}
.lds-ellipsis div:nth-child(4) {
  left: 56px;
  animation: lds-ellipsis3 1.8s infinite;
  animation-delay: 1.5s;
}
@@keyframes lds-ellipsis1 {
  0%, 20% {
    transform: scale(0);
  }
  100% {
    transform: scale(1);
  }
}
@@keyframes lds-ellipsis3 {
  0%, 20% {
    transform: scale(1);
  }
  100% {
    transform: scale(0);
  }
}
@@keyframes lds-ellipsis2 {
  0%, 20% {
    transform: translate(0, 0);
  }
  100% {
    transform: translate(24px, 0);
  }
}
.currenttimecolon{
    color:black;
    animation: blinkanimation 2s infinite;
}
@@keyframes blinkanimation{
    0%, 50%{
        color:#EEEEEE;
    }
    51%, 100%{
        color:black;
    }
}
.bottleicon {
  height: 25px;
  width: 25px;
  background-color: #bbb;
  border-radius: 50%;
  display: inline-block;
}.fieldstatus{
       transition:background-color 0.7s;
   }
   .disconnectedstatus{
       transition:background-color 0.7s,color 0.7s;
   }
   .disabledstatus{
       transition: content 0.5s;
   }
   .disabledstatus:hover:after{
       content: ' DISABLED';
   }
.rotationspin{
    animation-name:spin;
    animation-duration:2s;
    animation-iteration-count:1;
}
@@keyframes spin {
    from {
        transform:rotate(0deg);
    }
    to {
        transform:rotate(360deg);
    }
}
.pointstatus{
    transition: background-color 0.5s;
}
.appearingclass{
    animation-name: appearing;
    animation-iteration-count: 1;
    animation-duration: 1s;
}
@@keyframes appearing{
    0%{
        opacity: 0;
    }100%{
         opacity: 1;
     }
}
.disappearingclass{
    animation-name: disappearing;
    animation-iteration-count: 1;
    animation-duration: 1s;
}
@@keyframes disappearing{
    0%{
        opacity: 1;
    }100%{
         opacity: 0;
     }
}
.progressbar{
    transition: width 1s;
}
.statusshadow{
    transition: box-shadow 0.5s;
}
</style>
