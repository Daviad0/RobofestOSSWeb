        chat.on("scorekeeperStatus", function (status) {
            if (status == 0) {
                $("#scorekeeperstatusbar").html("<span class='fas fa-clock'> CURRENTLY BUSY</span>")
                $("#scorekeeperstatusbar").css("background-color", "gray")
            } else if (status == 1) {
                $("#scorekeeperstatusbar").html("<span class='fas fa-flag'> REVIEWING SCORES</span>")
                $("#scorekeeperstatusbar").css("background-color", "darkorange")
            } else if (status == 2) {
                $("#scorekeeperstatusbar").html("<span class='fas fa-thumbs-up'> OK</span>")
                $("#scorekeeperstatusbar").css("background-color", "forestgreen")
            }
        });
        // Start the connection.
        var fieldsub = 0;
        var lockedin = false;
